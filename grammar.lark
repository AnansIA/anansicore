%import common.WS_INLINE
%import common.NEWLINE
%ignore WS_INLINE
%ignore NEWLINE
COMMENT: /#[^\n]*/
%ignore COMMENT

VBAR: "|"
VBAR_WS: VBAR WS_INLINE*
%ignore VBAR_WS

ETIQUETA: /[A-Za-z-](?=\.)/
DOT: "."
COLON: ":"
ID: /[0-9]+(?:\.[0-9]+)*/

DELIMITED_EXPR: /\[\[[^\]]*\]\]/
PAYLOAD: DELIMITED_EXPR | /[^|\n]+/

payload: PAYLOAD | -> empty

token: ETIQUETA DOT ID (COLON payload)?